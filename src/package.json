  "keywords": [
    "eyemotion",
    "visual-storytelling",
    "film-production",
    "ai-editing",
    "cultural-verification",
    "ffz-framework",
    "intent-aware-ai",
    "adaptive-ecosystem"
  ],
  "author": {
    "name": "EyeMotion Team",
    "email": "hello@eyemotionai.com",
    "url": "https://eyemotionai.com"
  },
  "homepage": "https://eyemotionai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/eyemotion/eyemotion-ecosystem"

{
  "name": "eyemotion-ecosystem",
  "version": "1.0.0",
  "description": "EyeMotion Adaptive Visual Storytelling & Verification Ecosystem",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "deploy:production": "chmod +x deploy-production-eyemotionai.sh && ./deploy-production-eyemotionai.sh",
    "deploy:database": "tsx scripts/production-deployment.tsx",
    "health:check": "curl -f https://eyemotionai.com/health || echo 'Health check failed'",
    "monitor:start": "node monitoring/production-monitoring.js",
    "backup:create": "tsx scripts/backend-backup.tsx",
    "setup:ssl": "chmod +x ssl-setup-eyemotionai.sh && ./ssl-setup-eyemotionai.sh",
    "verify:deployment": "tsx scripts/verify-deployment.tsx",
    "test:production": "npm run health:check && npm run verify:deployment",
    "backup:full": "chmod +x scripts/complete-backup.sh && ./scripts/complete-backup.sh",
    "backup:incremental": "chmod +x scripts/incremental-backup.sh && ./scripts/incremental-backup.sh",
    "backup:database": "chmod +x scripts/database-backup.sh && ./scripts/database-backup.sh",
    "backup:manage": "chmod +x scripts/backup-manager.sh && ./scripts/backup-manager.sh",
    "backup:list": "chmod +x scripts/backup-manager.sh && ./scripts/backup-manager.sh list",
    "backup:status": "chmod +x scripts/backup-manager.sh && ./scripts/backup-manager.sh status",
    "backup:cleanup": "chmod +x scripts/backup-manager.sh && ./scripts/backup-manager.sh cleanup",
    "backup:schedule": "chmod +x scripts/backup-manager.sh && ./scripts/backup-manager.sh schedule"
  },
  "dependencies": {
